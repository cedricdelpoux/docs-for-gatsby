{
  "name": "google-docs-for-gatsby",
  "private": "true",
  "description": "Manage and publish your Gatsby website from Google Docs",
  "repository": "https://github.com/cedricdelpoux/google-docs-for-gatsby",
  "author": "CÃ©dric Delpoux <cedric.delpoux@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "google",
    "google-docs",
    "docs",
    "drive"
  ],
  "scripts": {
    "build:dev": "NODE_ENV=development webpack",
    "build": "NODE_ENV=production webpack",
    "certificate": "mkdir certs && mkcert -key-file ./certs/key.pem -cert-file ./certs/cert.pem localhost 127.0.0.1",
    "clean": "rm -rf dist",
    "cz": "git-cz",
    "deploy:dev": "yarn clean && yarn build:dev && npx clasp push",
    "deploy": "yarn clean && yarn build && npx clasp push",
    "dev": "yarn deploy:dev && yarn serve",
    "lint": "eslint src --ext .js",
    "serve": "webpack-dev-server",
    "setup": "clasp login && rm -f .clasp.json && clasp create --type docs --title 'Gatsby-source-google-docs Addon Test' --rootDir ./dist"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "antd": "^4.7.2",
    "gas-client": "^0.1.0",
    "moment": "^2.29.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@google/clasp": "^2.3.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dynamic-cdn-webpack-plugin": "^5.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-googleappsscript": "^1.0.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "gas-webpack-plugin": "^1.0.4",
    "google-apps-script-webpack-dev-server": "^1.0.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "module-to-cdn": "^3.1.5",
    "prettier": "^2.1.2",
    "terser-webpack-plugin": "^2.3.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
